<%= form_for event do |f| %>

<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :date %>
  <%= f.date_field :date %>
</p>

<p>
  <%= f.label :capacity %>
  <%= f.number_field :capacity %>
</p>

<p>
  <%= f.label :created_at %>
  <%= f.date_field :created_at %>
</p>

<p>
  <%= f.label :updated_at %>
  <%= f.date_field :updated_at %>
</p>

<p>
  <%= f.label :city %>
  <%= f.text_field :city %>
</p>

<% end %>
