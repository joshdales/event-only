<p>
  <%= link_to 'View Locations', locations_path %>
</p>

<% @locations.each do |current_location| %>
<%= render 'locations/single_location', location: current_location %>
<% end %>

<p>
  <%= link_to 'Events', events_path %>
</p>

<% @events.each do |current_event| %>
<%= render 'events/single_event', event: current_event %>
<% end %>
